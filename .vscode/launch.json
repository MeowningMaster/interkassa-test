{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "-A",
        "--watch=static/,routes/",
        "dev.ts"
      ],
      "attachSimplePort": 9229,
      "outputCapture": "std"
    }
  ]
}
