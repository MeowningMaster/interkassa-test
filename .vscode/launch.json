{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": [
        "run",
        "--inspect",
        "-A",
        "--unstable",
        "--import-map=import_map.json",
        "--no-check",
        "src/main.ts"
      ],
      "attachSimplePort": 9229,
      "port": 9229,
      "outputCapture": "std"
    }
  ]
}
